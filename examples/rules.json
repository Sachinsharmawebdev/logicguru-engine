[
  {
    "id": "section-rule",
    "useFiles": {
      "productFile": {
        "path": "/products/${get.data.productId}_slug.json",
        "variable": []
      }
    },
    "condition": {
      "and": [
        { "==": ["$get.source", "evectus"] }
      ]
    },
    "actions": [
      {
        "type": "update",
        "key": "$res.productId",
        "value": "$get.agentId",
        "returnKey":"res"
      },
      {
        "type": "update",
        "key": "$res.productName",
        "value": "$get.agentId"
      },
      {
        "type": "excludeVal",
        "key": "$res.addon",
        "exclude": "1234",
        "returnKey":"res"
      },
      {
        "type": "deleteKey",
        "key": "$res.addon",
        "returnKey":"res"
      },
      {
        "type": "log",
        "message": "res:${res}"
      }
    ]
  }
]