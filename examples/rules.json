[
  {
    "id": "section-rule",
    "useFiles": {
      "productFile": {
        "path": "${get.baseProductId}.json",
        "variable": []
      }
    },
    "condition": {
      "and": [
        { "==": ["$get.source", "evectus"] }
      ]
    },
    "actions": [
      {
        "type": "update",
        "key": "$res.productId",
        "value": "$get.agentId",
        "returnKey":"res"
      },
      {
        "type": "update",
        "key": "$res.productName",
        "value": "$get.agentId",
        "returnKey":"res"
      }
    ]
  }
]